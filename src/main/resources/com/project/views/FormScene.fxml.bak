<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import org.controlsfx.control.SearchableComboBox?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="655.0" prefWidth="1312.0"
            xmlns="http://javafx.com/javafx/22" fx:controller="com.project.controller.FormController">
    <children>
        <ScrollPane hbarPolicy="NEVER" prefHeight="617.0" prefWidth="543.0" style="-fx-background-color: transparent;">
            <content>
                <VBox fx:id="vboxMain" prefHeight="615.0" prefWidth="519.0" spacing="16.0">
                    <padding>
                        <Insets bottom="20.0" left="20.0" right="32.0" top="20.0"/>
                    </padding>
                    <HBox alignment="CENTER_LEFT" prefHeight="29.0" prefWidth="902.0" spacing="32.0">
                        <children>
                            <HBox alignment="CENTER_LEFT" prefHeight="29.0" prefWidth="315.0" spacing="4.0">
                                <children>
                                    <ImageView fx:id="iconFrom" fitHeight="30.0" fitWidth="30.0" pickOnBounds="true"
                                               preserveRatio="true">
                                        <image>
                                            <Image url="@../../../../resources/com/project/img/airplane-takeoff.png"/>
                                        </image>
                                    </ImageView>
                                    <Label text="Kota Asal"/>
                                </children>
                            </HBox>
                            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="315.0" spacing="4.0">
                                <children>
                                    <ImageView fx:id="iconDestination" fitHeight="29.0" fitWidth="39.0"
                                               pickOnBounds="true" preserveRatio="true">
                                        <image>
                                            <Image url="@../../../../resources/com/project/img/airplane-landing.png"/>
                                        </image>
                                    </ImageView>
                                    <Label text="Kota Asal"/>
                                </children>
                            </HBox>
                        </children>
                    </HBox>
                    <HBox prefHeight="40.0" prefWidth="664.0" spacing="32.0">
                        <children>
                            <SearchableComboBox fx:id="cbFrom" minHeight="40.0" prefHeight="25.0" prefWidth="315.0"
                                                promptText="Kota Asal"/>
                            <SearchableComboBox fx:id="cbDestination" minHeight="40.0" prefHeight="25.0"
                                                prefWidth="315.0" promptText="Kota Tujuan"/>
                        </children>
                    </HBox>
                    <Label graphicTextGap="8.0" text="Tanggal Keberangkatan">
                        <graphic>
                            <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                <image>
                                    <Image url="@../../../../resources/com/project/img/calendar.png"/>
                                </image>
                            </ImageView>
                        </graphic>
                    </Label>
                    <DatePicker fx:id="datePickerDeparture" minHeight="40.0" prefHeight="40.0" prefWidth="664.0"/>
                    <Label graphicTextGap="8.0" text="Kelas Kursi">
                        <graphic>
                            <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                <image>
                                    <Image url="@../../../../resources/com/project/img/seat.png"/>
                                </image>
                            </ImageView>
                        </graphic>
                    </Label>
                    <ChoiceBox fx:id="choiceSeat" minHeight="32.0" prefWidth="150.0" value="Any"/>
                    <VBox fx:id="vboxPassengerField" prefWidth="379.0" spacing="16.0">
                        <children>
                            <HBox spacing="16.0">
                                <children>
                                    <VBox prefHeight="65.0" spacing="8.0">
                                        <children>
                                            <Label text="Nama Penumpang 1"/>
                                            <TextField minHeight="40.0" prefHeight="40.0" prefWidth="303.0"
                                                       promptText="Nama penumpang"/>
                                        </children>
                                    </VBox>
                                    <VBox prefHeight="65.0" spacing="8.0">
                                        <children>
                                            <Label text="Usia Penumpang 1"/>
                                            <TextField minHeight="40.0" prefHeight="40.0" prefWidth="303.0"
                                                       promptText="Nama penumpang"/>
                                        </children>
                                    </VBox>
                                </children>
                            </HBox>
                        </children>
                    </VBox>
                    <Button mnemonicParsing="false" onAction="#onClickAddPassenger" text="Tambah penumpang...">
                        <graphic>
                            <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                                <image>
                                    <Image url="@../../../../resources/com/project/img/person-add.png"/>
                                </image>
                            </ImageView>
                        </graphic>
                    </Button>
                    <Button onAction="#onSubmit" prefHeight="25.0" prefWidth="472.0" text="Cari TIket"/>
                </VBox>
            </content>
        </ScrollPane>
        <ScrollPane hbarPolicy="NEVER" layoutX="556.0" layoutY="1.0" prefHeight="583.0" prefWidth="751.0"
                    style="-fx-background-color: transparent;">
            <content>
                <VBox fx:id="vBoxSchedule" prefHeight="585.0" prefWidth="749.0" spacing="16.0">
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <padding>
                        <Insets right="16.0" top="16.0"/>
                    </padding>
                    <children>
                        <HBox alignment="CENTER_LEFT" prefHeight="29.0" prefWidth="847.0" spacing="16.0"
                              style="-fx-border-width: 1px; -fx-border-color: grey; -fx-border-radius: 6px;">
                            <children>
                                <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true"/>
                                <VBox alignment="CENTER_LEFT" prefHeight="37.0" prefWidth="149.0">
                                    <children>
                                        <Label text="00 Januari 0000">
                                            <font>
                                                <Font size="14.0"/>
                                            </font>
                                        </Label>
                                        <Label text="00.00 - 00.00">
                                            <font>
                                                <Font size="14.0"/>
                                            </font>
                                        </Label>
                                        <Label text="Travel Time:  0 hr 0 min"/>
                                    </children>
                                </VBox>
                                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="27.0" prefWidth="147.0"
                                       text="IATA - IATA">
                                    <font>
                                        <Font size="18.0"/>
                                    </font>
                                </Label>
                                <Label fx:id="price" alignment="CENTER_RIGHT" prefHeight="37.0" prefWidth="223.0"
                                       text="IDR 0.000.000" textAlignment="RIGHT">
                                    <font>
                                        <Font size="18.0"/>
                                    </font>
                                </Label>
                                <Button mnemonicParsing="false" prefHeight="37.0" prefWidth="99.0" text="Button"/>
                            </children>
                            <padding>
                                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                            </padding>
                        </HBox>
                    </children>
                </VBox>
            </content>
        </ScrollPane>
    </children>
</AnchorPane>
